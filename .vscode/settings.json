{
	"stylelint.syntax": "scss",
	"scss.showErrors": true,
	"stylelint.configBasedir": "/",
	"stylelint.validate": ["html", "css", "sass", "scss", "less", "styl"],

	"files.associations": {
		"*.xml": "html",
		"*.svg": "html"
	},
	"stylelint.configFile": ".stylelintrc.js",
	"stylelint.enable": true,
	"sass.lint.duplicateProperties": "error",
	"scss.lint.duplicateProperties": "error",
	"css.lint.duplicateProperties": "error",
	"less.lint.duplicateProperties": "error",
	"css.validate": false,
	"less.validate": false,
	"scss.validate": false,
	// "css.lint.duplicateProperties": "ignore",
	// "css.lint.duplicateProperties": "error",
	// "css.lint.duplicateProperties": "warning",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": true,
		"source.organizeImports": true
	},
